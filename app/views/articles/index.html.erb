
<%= link_to 'New article', new_article_path %>
<h4>Articles:</h4>

<% @articles.each do |article|%>
<p><%= article.title %></p>
<p><%= article.body%></p>
<%= link_to 'Show', article_path(article) %>|
<%= link_to 'Edit', edit_article_path(article) %>|
<%= button_to 'Delete', article_path(article),data:{turbo: false}, method: :delete %>
<% end %>